<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified"
           targetNamespace="http://nite.sourceforge.net/">

    <xs:element name="stream">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="s" maxOccurs="unbounded" form="unqualified">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="token" maxOccurs="unbounded" form="unqualified" >
                                <xs:complexType>
                                    <xs:simpleContent>
                                    	<xs:extension base="xs:string">
                                    	<xs:attribute name="id" type="xs:string" use="required" form="qualified"/>
                                    	</xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:string" use="required" form="qualified"/>
		        <xs:attribute name="np-complete" type="xs:boolean" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="id" type="xs:string" use="required" form="qualified"/>
        </xs:complexType>
    </xs:element>                                  
                               
</xs:schema>
